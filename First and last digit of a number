import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    System.out.println("ENTERED NUMBER="+n);
	    int firstDigit=0;
	    int lastDigit=0;
	    lastDigit=n%10;
	    System.out.println("last digit is="+lastDigit);
	    while(n!=0){
	      firstDigit=n%10;
	      n=n/10;}
		System.out.println("first digit is="+firstDigit);
	}
}
