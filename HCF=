import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter 1 number:");
        int N=sc.nextInt();
        System.out.print("enter 2 number:");
        int M=sc.nextInt();
        int HCF=0;
        for(int i=1;i<=N||i<=M;i++){
            if(N%i==0&&M%i==0)
                HCF=i;
        }
        System.out.print("HCF is:"+HCF);
    }
}
