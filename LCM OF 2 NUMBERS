import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the value of a=");
        int a=sc.nextInt();
        System.out.print("enter the value of b=");
        int b=sc.nextInt();
        int lcm=(a>b)?a:b;
        while(true){
            if(lcm%a==0&&lcm%b==0){
                System.out.print("LCM is=" +lcm);
                break;
            }
            ++lcm;

        }
    }
}
