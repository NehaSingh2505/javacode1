import java.util.*;
public class Main
{
    
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	     int rev=0;
	    int rem;
	    int temp;
	    temp=a;
	    while(temp!=0){
	        rem=temp%10;
	        rev=rev*10+rem;
	        temp/=10;
	    };
	    if(a==rev){
	        System.out.println(a + "is palindrome" );
	    }
	    else
		    System.out.println(a + "is not palindrome" );
	}
}

