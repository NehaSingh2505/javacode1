import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the string=");
        String s1= sc.nextLine();
        int count=0;
        for(int i=0;i<s1.length();i++){
            count++;

        }
        System.out.print("count of words="+ count);

    }
}
