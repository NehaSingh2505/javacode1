import java.util.*;
class Main{
    public static void main(String[]args){
        int a;int rev=0;
        Scanner sc=new Scanner(System.in);
        System.out.print("enter number=");
        a=sc.nextInt();

        while(a!=0){
            int rem=a%10;
            rev=rev*10+rem;
            a=a/10;
        }
        System.out.println("reverse of the number is ="+rev);

    }
}
