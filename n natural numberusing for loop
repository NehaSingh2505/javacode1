import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.print("enter the value of a=");
	    int a=sc.nextInt(); 
		System.out.println("natural numbers are=");
		for(int n=1;n<=a;n++){
		    System.out.println(n);
		}
	}
}
