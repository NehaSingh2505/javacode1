import java.util.*;
class Main
{
    public static void main(String[]args){
        int a;
        System.out.println("enter number:");
        Scanner b=new Scanner(System.in);
        a=b.nextInt();
        if(a%2==0)
        {
            System.out.println("even number");
        }
        else{
            System.out.println("odd number");
        }
    }
}
