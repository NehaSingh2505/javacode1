import java.util.Scanner;
public class Main
    {
        public static void main (String[] args) {
        Scanner sc=new Scanner (System.in);
        int a=sc.nextInt();
        if(a%1==0&&a%a==0){
            System.out.println("prime number");
        }
        else{
            System.out.println("not a prime number");
        }
    }
}
