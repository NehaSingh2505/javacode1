import java.util.*;
public class Main
{
public static void main(String[]args){
  int a;int rev=0;int rem;
  Scanner sc=new Scaner(System.in);
  System.out.print("enter number=");
  a=sc.nextInt();
  while(a!=0){
     rem=a%10;
     rev=rev*10+rem;
     a=a/10;
  }
  System.out.println("reverse of the given number="+rev);
}
}
