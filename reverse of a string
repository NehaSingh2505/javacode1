import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the string=");
        String s1= sc.next();
        String rev="";
        for(int i=0;i<s1.length();i++){
            rev=s1.charAt(i)+rev;

        }
        System.out.print("reverse of the string="+rev);

    }
}
