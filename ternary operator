import java.util.*;
public class Main
{
    public class void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a= sc.nextInt();
        int b= sc.nextInt();
        int c= sc.nextInt();
        int max;
        max=(a>b)?(a>c?a:c):(b>c?b:c);
        System.out.println("greatest number="+max);
        
}
}
